{
  "env": {
    "cjs": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "12.22",
            "browsers": [
              "last 4 versions",
              "not ie 1-11",
              "not ie_mob 1-11",
              "not android > 0"
            ]
          }
        }]
      ]
    },

    "esm": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "12.22",
            "browsers": [
              "last 4 versions",
              "not ie 1-11",
              "not ie_mob 1-11",
              "not android > 0"
            ]
          },
          "modules": false
        }]
      ]
    },

    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "12.22"
          }
        }]
      ]
    }
  }
}
